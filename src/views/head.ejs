<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="Congregação Cristã no Brasil - <%= regional.name %>">
    <meta name="author" content="">
    <title><%= regional.name %> - Registros de Visitas</title>
    <link href="/static/css/styles.css" rel="stylesheet" />
    <link href="/static/css/custom/custom.css" rel="stylesheet" />
    <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Manrope:wght@400;500;600;700;800&family=Playfair+Display:wght@600;700&display=swap">
    <link rel="apple-touch-icon" sizes="180x180" href="/static/images/icon.png" type="image/png">
    <link rel="icon" href="/static/images/logo_ccb_white.png" type="image/png">
    <link rel="manifest" href="/static/manifest.json">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.4/css/all.min.css">
        <!-- Load core libraries with multiple-CDN fallback and final local fallback -->
        <script>
            (function(){
                function load(src, cb, errCb){
                    var s = document.createElement('script');
                    s.src = src; s.async = false;
                    s.onload = function(){ cb && cb(); };
                    s.onerror = function(){ errCb && errCb(); };
                    document.head.appendChild(s);
                }

                function trySequence(sources, onReady, onFail){
                    var i=0;
                    function next(){
                        if(i>=sources.length){ onFail && onFail(); return; }
                        load(sources[i], onReady, function(){ i++; next(); });
                    }
                    next();
                }

                // jQuery first (required by many other libs)
                trySequence([
                    'https://code.jquery.com/jquery-3.6.0.min.js',
                    'https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.0/jquery.min.js',
                    'https://cdn.jsdelivr.net/npm/jquery@3.6.0/dist/jquery.min.js'
                ], function(){
                    console.log('jquery loaded', typeof window.jQuery);
                    // Then DataTables and SweetAlert
                    trySequence([
                        'https://cdn.datatables.net/1.13.6/js/jquery.dataTables.min.js',
                        'https://cdn.datatables.net/1.10.25/js/jquery.dataTables.min.js'
                    ], function(){ console.log('datatables loaded'); }, function(){
                        // local fallback
                        load('/static/js/jquery.dataTables.min.js', function(){ console.log('datatables local loaded'); });
                    });

                    trySequence([
                        'https://cdn.jsdelivr.net/npm/sweetalert2@11',
                        'https://cdn.jsdelivr.net/npm/sweetalert2@10'
                    ], function(){ console.log('sweetalert loaded'); }, function(){
                        load('/static/js/sweetalert2.min.js', function(){ console.log('sweetalert local loaded'); });
                    });

                }, function(){
                    // jQuery failed from CDNs -> try local
                    load('/static/js/jquery-3.6.0.min.js', function(){ console.log('jquery local loaded'); }, function(){ console.warn('jquery failed to load from all sources'); });
                });

                // Chart.js (independent of jQuery) and plugin
                trySequence([
                    'https://cdnjs.cloudflare.com/ajax/libs/Chart.js/3.1.1/chart.min.js',
                    'https://cdn.jsdelivr.net/npm/chart.js@3.9.1/dist/chart.min.js',
                    'https://cdn.jsdelivr.net/npm/chart.js@3/dist/chart.min.js'
                ], function(){
                    console.log('chart.js loaded', typeof window.Chart);
                    // plugin
                    trySequence([
                        'https://cdn.jsdelivr.net/npm/chartjs-plugin-datalabels@2.0.0',
                        'https://cdn.jsdelivr.net/npm/chartjs-plugin-datalabels@2'
                    ], function(){ console.log('chart datalabels loaded'); }, function(){
                        load('/static/js/chartjs-plugin-datalabels.min.js', function(){ console.log('chart datalabels local loaded'); });
                    });
                }, function(){
                    load('/static/js/chart.min.js', function(){ console.log('chart local loaded'); });
                });

                // Bootstrap bundle (depends only on native JS)
                trySequence([
                    'https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js',
                    'https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/5.1.3/js/bootstrap.bundle.min.js'
                ], function(){ console.log('bootstrap loaded'); }, function(){
                    load('/static/js/bootstrap.bundle.min.js', function(){ console.log('bootstrap local loaded'); });
                });

            })();
        </script>
    <link rel="stylesheet" href="https://cdn.datatables.net/1.13.6/css/jquery.dataTables.min.css">
    <!-- Local fallback for DataTables CSS -->
    <link rel="stylesheet" href="/static/css/jquery.dataTables.min.css">
</head> 
